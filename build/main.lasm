    ret


; arguments
; initial number <- 1
; number <- 0
return:
    drop
    ret
addiUntil:
    swap
addiUntilLoop:
    push int 1
    addi
    dup 1
    dup 1
    equ
    jmp_if return
    jmp addiUntilLoop

start:
    push int 65 ; 'A' in decimal
    call putc
    push int 0
    push int 10
    call addiUntil
    push float 0.10
    halt